{
	"name": "studiokit-auth-js",
	"version": "0.2.3",
	"scripts": {
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
		"lint": "eslint --ext=js ./*.js src test",
		"fix": "npm run lint -- --fix",
		"test": "cross-env BABEL_ENV=test jest",
		"test:watch": "cross-env BABEL_ENV=test jest --watch",
		"coverage": "cross-env BABEL_ENV=test jest --coverage",
		"coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"build": "babel src -d lib",
		"build:watch": "babel src --watch -d lib",
		"prepare": "npm run build"
	},
	"description": "Authentication flow for Studio apps at Purdue",
	"main": "lib/index.js",
	"repository": "purdue-tlt/studiokit-auth-js",
	"license": "MIT",
	"files": [
		"lib"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.0.0",
		"babel-eslint": "7.2.3",
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.0",
		"babel-runtime": "^6.26.0",
		"babel-template": "^6.2.0",
		"babel-types": "^6.2.0",
		"coveralls": "^3.0.2",
		"cross-env": "^5.0.5",
		"eslint": "4.10.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-config-studiokit": "^0.2.1",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-redux-saga": "^0.10.0",
		"fetch-mock": "^7.2.5",
		"flow-bin": "^0.87.0",
		"husky": "^1.2.0",
		"immutable": "^3.8.2",
		"isomorphic-fetch": "^2.2.1",
		"jest": "^23.1.0",
		"lint-staged": "^8.1.0",
		"prettier": "^1.14.3"
	},
	"dependencies": {
		"redux-saga": "^0.16.2",
		"studiokit-net-js": "^1.5.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run test"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	}
}
